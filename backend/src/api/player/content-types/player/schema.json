{
  "kind": "collectionType",
  "collectionName": "players",
  "info": {
    "singularName": "player",
    "pluralName": "players",
    "displayName": "Player"
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "ranking": {
      "type": "integer"
    },
    "faction": {
      "type": "string"
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "player"
    },
    "matches": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::match.match",
      "mappedBy": "player2"
    },
    "leagues": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::league.league",
      "mappedBy": "players"
    },
    "league_players": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::league-player.league-player",
      "mappedBy": "player"
    }
  }
}
