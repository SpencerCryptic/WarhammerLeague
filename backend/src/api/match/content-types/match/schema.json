{
  "kind": "collectionType",
  "collectionName": "matches",
  "info": {
    "singularName": "match",
    "pluralName": "matches",
    "displayName": "Match",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "league": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::league.league",
      "inversedBy": "matches"
    },
    "leaguePlayer1": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::league-player.league-player",
      "inversedBy": "firstPlayerMatches"
    },
    "leaguePlayer2": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::league-player.league-player",
      "inversedBy": "secondPlayerMatches"
    },
    "leaguePlayer1List": {
      "type": "text"
    },
    "leaguePlayer2List": {
      "type": "text"
    },
    "statusMatch": {
      "type": "enumeration",
      "enum": [
        "upcoming",
        "planned",
        "played",
        "abandoned"
      ]
    },
    "leaguePlayer1Score": {
      "type": "integer"
    },
    "leaguePlayer2Score": {
      "type": "integer"
    },
    "leaguePlayer1Result": {
      "type": "integer"
    },
    "leaguePlayer2Result": {
      "type": "integer"
    },
    "matchUID": {
      "type": "uid"
    },
    "leaguePlayer1BonusPoints": {
      "type": "json",
      "default": {
        "lostButScored50Percent": false,
        "scoredAllPrimaryObjectives": false
      }
    },
    "leaguePlayer2BonusPoints": {
      "type": "json",
      "default": {
        "lostButScored50Percent": false,
        "scoredAllPrimaryObjectives": false
      }
    },
    "leaguePlayer1LeaguePoints": {
      "type": "integer",
      "default": 0
    },
    "leaguePlayer2LeaguePoints": {
      "type": "integer",
      "default": 0
    },
    "matchResult": {
      "type": "enumeration",
      "default": "unplayed",
      "enum": [
        "player1_win",
        "player2_win",
        "draw",
        "unplayed"
      ]
    },
    "round": {
      "type": "integer"
    }
  }
}
