{
  "kind": "collectionType",
  "collectionName": "league_players",
  "info": {
    "singularName": "league-player",
    "pluralName": "league-players",
    "displayName": "LeaguePlayer",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "player": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::player.player",
      "inversedBy": "league_players"
    },
    "league": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::league.league",
      "inversedBy": "league_players"
    },
    "faction": {
      "type": "string"
    },
    "wins": {
      "type": "integer"
    },
    "draws": {
      "type": "integer"
    },
    "losses": {
      "type": "integer"
    },
    "rankingPoints": {
      "type": "integer"
    },
    "leagueName": {
      "type": "string",
      "required": true
    },
    "goodFaithAccepted": {
      "type": "boolean"
    },
    "playList": {
      "type": "text"
    },
    "firstPlayerMatches": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::match.match",
      "mappedBy": "leaguePlayer1"
    },
    "secondPlayerMatches": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::match.match",
      "mappedBy": "leaguePlayer2"
    }
  }
}
