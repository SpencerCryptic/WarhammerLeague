{
  "kind": "collectionType",
  "collectionName": "league_players",
  "info": {
    "singularName": "league-player",
    "pluralName": "league-players",
    "displayName": "LeaguePlayer",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "player": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::player.player",
      "inversedBy": "league_players"
    },
    "league": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::league.league",
      "inversedBy": "league_players"
    },
    "faction": {
      "type": "enumeration",
      "enum": [
        "Stark",
        "Lannister",
        "Free Folk",
        "Nights Watch",
        "Baratheon",
        "Targaryen",
        "Greyjoy",
        "Martell",
        "Bolton",
        "Brotherhood Without Banners",
        "Neutral",
        "Dark Angels",
        "White Scars",
        "Imperial Fists",
        "Blood Angels",
        "Raven Guard",
        "Ultramarines",
        "Salamanders",
        "Iron Hands",
        "Sons of Horus",
        "World Eaters",
        "Emperor's Children",
        "Death Guard",
        "Thousand Sons",
        "Word Bearers",
        "Iron Warriors",
        "Night Lords",
        "Alpha Legion"
      ]
    },
    "wins": {
      "type": "integer"
    },
    "draws": {
      "type": "integer"
    },
    "losses": {
      "type": "integer"
    },
    "rankingPoints": {
      "type": "integer"
    },
    "leagueName": {
      "type": "string",
      "required": true
    },
    "goodFaithAccepted": {
      "type": "boolean"
    },
    "armyLists": {
      "type": "json",
      "default": []
    },
    "firstName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "firstPlayerMatches": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::match.match",
      "mappedBy": "leaguePlayer1"
    },
    "secondPlayerMatches": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::match.match",
      "mappedBy": "leaguePlayer2"
    }
  }
}
