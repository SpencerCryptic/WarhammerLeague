{
  "kind": "collectionType",
  "collectionName": "leagues",
  "info": {
    "singularName": "league",
    "pluralName": "leagues",
    "displayName": "League",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "blocks"
    },
    "statusleague": {
      "type": "enumeration",
      "enum": [
        "planned",
        "ongoing",
        "completed"
      ]
    },
    "matches": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::match.match",
      "mappedBy": "league"
    },
    "leaguePassword": {
      "type": "string"
    },
    "league_players": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::league-player.league-player",
      "mappedBy": "league"
    },
    "createdByUser": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "leagues"
    },
    "gameSystem": {
      "type": "enumeration",
      "enum": [
        "Warhammer: 40,000",
        "Warhammer: Age of Sigmar",
        "Warhammer: Kill Team",
        "Warhammer: Warcry",
        "Warhammer: Necromunda",
        "A Song of Ice and Fire",
        "Middle Earth SBG",
        "Marvel Crisis Protocol"
      ]
    }
  }
}
